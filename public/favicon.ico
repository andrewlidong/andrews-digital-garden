<svg xmlns="http://www.w3.org/2000/svg" width="64" height="64" viewBox="0 0 64 64">
  <!-- Terminal background -->
  <rect width="64" height="64" rx="8" fill="#121212" />
  
  <!-- Terminal header -->
  <rect x="4" y="4" width="56" height="8" rx="2" fill="#333333" />
  
  <!-- Terminal window buttons -->
  <circle cx="8" cy="8" r="2" fill="#FF5F56" />
  <circle cx="16" cy="8" r="2" fill="#FFBD2E" />
  <circle cx="24" cy="8" r="2" fill="#27C93F" />
  
  <!-- Terminal content area -->
  <rect x="4" y="16" width="56" height="44" rx="2" fill="#1E1E1E" />
  
  <!-- Terminal prompt symbol -->
  <text x="8" y="32" font-family="monospace" font-size="14" fill="#4CAF50">$</text>
  
  <!-- Initials "AD" -->
  <text x="18" y="32" font-family="monospace" font-size="14" fill="#4CAF50">AD</text>
  
  <!-- Cursor -->
  <rect x="38" y="24" width="2" height="14" fill="#4CAF50">
    <animate attributeName="opacity" values="1;0;1" dur="1.5s" repeatCount="indefinite" />
  </rect>
</svg> 